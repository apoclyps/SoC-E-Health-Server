<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>E-Health - Welcome</display-name>
	<welcome-file-list>
		<welcome-file>FeedController</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>FeedController</servlet-name>
		<servlet-class>uk.co.kyleharrison.jobseeker.servlets.RequestController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FeedController</servlet-name>
		<url-pattern>/FeedController</url-pattern>
		<url-pattern>/FeedController/*</url-pattern>
	</servlet-mapping>
	<error-page>
		<error-code>404</error-code>
		<location>/EHealth/Error/error.jsp</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/EHealth/Error/error.jsp</location>
	</error-page>
</web-app>